<% layout('../layout/templates/seller/boilerplate') -%>
<div class="container col-6 offset-3">
    <!-- Age Group -->
<div class="form-group">
    <label for="ageGroup">Age Group</label>
    <input type="text" class="form-control" id="ageGroup" name="ageGroup" placeholder="Enter age group">
</div>

<!-- Category -->
<div class="form-group">
    <label for="category">Category</label>
    <input type="text" class="form-control" id="category" name="category" placeholder="Enter toy category">
</div>

<!-- Material -->
<div class="form-group">
    <label for="material">Material</label>
    <input type="text" class="form-control" id="material" name="material" placeholder="Enter material">
</div>

<!-- Educational Value -->
<div class="form-group">
    <label for="educationalValue">Educational Value</label>
    <input type="text" class="form-control" id="educationalValue" name="educationalValue" placeholder="Enter educational value">
</div>

<!-- Interactive Features -->
<div class="form-group">
    <label for="interactiveFeatures">Interactive Features</label>
    <input type="text" class="form-control" id="interactiveFeatures" name="interactiveFeatures" placeholder="Enter interactive features">
</div>

<!-- Battery Powered -->
<div class="form-group">
    <label for="batteryPowered">Battery Powered</label>
    <select class="form-control" id="batteryPowered" name="batteryPowered">
        <option value="true">Yes</option>
        <option value="false">No</option>
    </select>
</div>

<!-- Characters -->
<div class="form-group">
    <label for="characters">Characters</label>
    <input type="text" class="form-control" id="characters" name="characters" placeholder="Enter characters (comma-separated)">
</div>

<!-- Brand -->
<div class="form-group">
    <label for="brand">Brand</label>
    <input type="text" class="form-control" id="brand" name="brand" placeholder="Enter brand">
</div>

<button type="submit" class="btn btn-primary">Submit</button>

</div>